//Question 1: Reverse an Array Without Using Built-in Functions

const reverseArray = (arr) => {
    let i = 0;
    let j = arr.length - 1

    while (i < j) {
        let temp = arr[i]
        arr[i] = arr[j]
        arr[j] = temp
        i++
        j--
    }
    return arr
}

let arr = [1, 2, 3, 4, 5]
console.log(reverseArray(arr));

let arr1 = [10, 20, 30, 40]
console.log(reverseArray(arr1));

let arr2 = [7, 8, 9]
console.log(reverseArray(arr));
